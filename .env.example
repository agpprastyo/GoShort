# Server Configuration
SERVER_PORT=8080
SERVER_READ_TIMEOUT=10s
SERVER_WRITE_TIMEOUT=10s
SERVER_BASE_URL=http://localhost:8080

# PostgreSQL Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_db_password
DB_NAME=goshort
DB_SSLMODE=disable
DB_MAX_OPEN_CONN=10
DB_MAX_IDLE_CONN=5
DB_MAX_LIFETIME=5m

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password
REDIS_DB=0
REDIS_POOL_SIZE=10

# Logger Configuration
LOG_LEVEL=info
LOG_JSON=false

# Swagger Auth
SWAGGER_AUTH_USERNAME=your_swagger_username
SWAGGER_AUTH_PASSWORD=your_swagger_password

# Basic Auth
BASIC_AUTH_USERNAME=your_basic_auth_username
BASIC_AUTH_PASSWORD=your_basic_auth_password